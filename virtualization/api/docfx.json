{
  "build": {
    "content": [
      {
        "files": ["**/**.md", "**/**.yml"],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [], 
    "globalMetadata": {
      "disable_contribution_on_translation_quality": ["MTE95","MTE75","MT"],
      "breadcrumb_path": "/virtualization/toc.json",
      "extendBreadcrumb": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "virtualization/api"
  }
}
