{
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ],
      "breadcrumb_path": "/virtualization/toc.json",
      "extendBreadcrumb": true, 
      "feedback_system": "GitHub", 
      "feedback_github_repo": "MicrosoftDocs/Virtualization-Documentation.zh-cn", 
      "feedback_product_url": "https://github.com/MicrosoftDocs/Virtualization-Documentation/issues"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "virtualization/api"
  }
}
